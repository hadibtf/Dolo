<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.dreamolo">

    <application
        android:name=".DreamApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".RankYourOloActivity" />

        <activity android:name=".PaymentActivity" />

        <activity android:name=".FirstWishActivity" />

        <activity android:name=".WishDetailActivity" />

        <activity android:name=".DescribeWishActivity" />

        <activity android:name=".ChoosingOloActivity" />

        <activity android:name=".EnterOloCodeActivity" />

        <activity android:name=".SearchingOloActivity" />

        <activity android:name=".RegistrationActivity" />

        <activity android:name=".DreamDescriptionActivity" />

        <activity android:name=".SearchResultActivity" />

        <activity android:name=".FiltersActivity" />

        <activity android:name=".ChooseDreamActivity" />

        <activity android:name=".DesignedDeramsOrMakeYourWishActivity" />
    </application>

</manifest>